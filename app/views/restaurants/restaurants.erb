<% if logged_in? %>

<p><a href="/logout">Log Out</a> <a href="/orders">Orders History</a></p>

<h3>Start a new Order from:</h3>
<br>
<% @restaurants.each do |restaurant|%>
<h2><a href="/restaurants/<%= restaurant.id %>"><%= restaurant.name %></a>
 ------ <%= restaurant.genre.capitalize %></h2>

<% end %>

<% else %>
<%= redirect to "/failure" %>
<% end %>